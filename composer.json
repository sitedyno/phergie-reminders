{
    "name": "sitedyno/phergie-reminders",
    "description": "Phergie plugin to remind you when the pizza is done.",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Heath Nail",
            "email": "sitedyno@gmail.com",
            "homepage": "https://github.com/sitedyno",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "sitedyno@gmail.com",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/sitedyno/phergie-reminders/issues",
        "source": "https://github.com/sitedyno/phergie-reminders"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~1",
        "phergie/phergie-irc-plugin-react-command": "~1.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "phake/phake": "2.1.0"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Sitedyno\\Phergie\\Plugin\\Reminders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sitedyno\\Phergie\\Tests\\Plugin\\Reminders\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
